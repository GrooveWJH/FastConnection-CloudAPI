# MQTT broker addresses exposed to browser clients
MQTT_TCP_URL=tcp://127.0.0.1:1883
MQTT_WS_URL=ws://127.0.0.1:8083/mqtt

# Credentials propagated into the login page
MQTT_USERNAME=admin
MQTT_PASSWORD=public

# Optional overrides for WebSocket port/path (usually leave empty)
MQTT_WS_PORT=8083
MQTT_WS_PATH=/mqtt

# Web UI binding on the host
WEB_BIND_HOST=127.0.0.1
WEB_BIND_PORT=3200
